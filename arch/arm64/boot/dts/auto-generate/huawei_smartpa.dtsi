/ {
	maxim_ioctl {
		compatible = "huawei,maxim_ioctl";
		status = "disabled";
	};

	amba {
		i2c3: i2c@FDF0C000 {
			max98925_smartpa_l:smartpa@31 {
				compatible = "huawei,max98925";
				smartpa_type = <0>; //left smartPA
				reg = <0x31>;
				gain = <0x14>;
				gain_incall = <0x14>;
				status = "disabled";
			};
			max98925_smartpa_r:smartpa@34 {
				compatible = "huawei,max98925";
				smartpa_type = <1>; //right smartPA
				reg = <0x34>;
				gain = <0x34>;
				gain_incall = <0x36>;
				status = "disabled";
			};
			tas2560_smartpa_l:smartpa@4F {
				compatible = "huawei,tas2560";
				smartpa_type = <0>; //left smartPA
				reg = <0x4F>;
				gpio_reset = <0>;
				status = "disabled";
			};
			tas2560_smartpa_r:smartpa@4C {
				compatible = "huawei,tas2560";
				smartpa_type = <1>; //right smartPA
				reg = <0x4C>;
				gpio_reset = <0>;
				status = "disabled";
			};
		};
	};
};
